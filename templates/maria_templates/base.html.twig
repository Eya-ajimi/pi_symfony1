<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}fadit!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
		{% block stylesheets %}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
			<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
			<link href="{{ asset('vendors/nprogress/nprogress.css') }}" rel="stylesheet">
			<link href="{{ asset('build/css/custom.min.css') }}" rel="stylesheet">
			<link href="{{ asset('build/css/custom.css') }}" rel="stylesheet">
			<link href="{{ asset('build/css/maria/side_bar.css') }}" rel="stylesheet">
			<link href="{{ asset('build/css/maria/base_style.css') }}" rel="stylesheet">
			<link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;700&display=swap" rel="stylesheet">
			<style></style>
		{% endblock %}
	</head>
	<body class="nav-md">
		<div class="container body">
			<div class="main_container">
				{% include 'maria_templates/bars.html.twig' %}

				<div class="right_col" role="main">
					{% for message in app.flashes('success') %}
						<div class="alert alert-success">{{ message }}</div>
					{% endfor %}
					{% block body %}{% endblock %}
				</div>
			</div>
		</div>

		{% block javascripts %}
			<!-- Load jQuery first -->
			<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
			<!-- Then Bootstrap -->
			<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
			<!-- Then your custom scripts -->
			<script src="{{ asset('vendors/jquery/dist/jquery.min.js') }}"></script>

			<script src="{{ asset('build/js/maria/test.js') }}"></script>
		{% endblock %}
	</body>
</html>
